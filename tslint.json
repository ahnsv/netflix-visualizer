{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            false
        ],
        "semicolon": false,
        "trailing-comma": [
            false,
            "error"
        ],
        "ordered-imports": false,
        "prefer-const": false,
        "forin": false,
        "no-console": false,
        "arrow-parens": false,
        "interface-name": false
    },
    "rulesDirectory": []
}